<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Reserve_Fund_Check</fullName>
    <active>true</active>
    <errorConditionFormula>Hourly_Rate__c  * Hours__c &gt;  Maintenance_Request__r.Property__r.Management_Contract__r.Reserve_Fund__c  &amp;&amp;  Job_Awarded__c  = true</errorConditionFormula>
    <errorMessage>This job cannot be awarded because the quote price exceeds the reserve fund for this property.</errorMessage>
</ValidationRule>
